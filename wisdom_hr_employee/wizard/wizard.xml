<odoo>
    <record id="view_volunteer_data_wizard_form" model="ir.ui.view">
        <field name="name">volunteer.data.wizard.form</field>
        <field name="model">volunteer.data.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Volunteer Data">
                <group>
                    <field name="file_data" filename="file_name"/>
                </group>
                <footer>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                    <button string="Import" type="object" name="action_import" class="btn-primary"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_volunteer_data_wizard" model="ir.actions.act_window">
        <field name="name">Import Volunteer Data</field>
        <field name="res_model">volunteer.data.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>