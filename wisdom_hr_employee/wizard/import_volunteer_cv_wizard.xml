<odoo>
    <record id="view_import_volunteer_wizard" model="ir.ui.view">
        <field name="name">res.users.import.wizard.form</field>
        <field name="model">res.users.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Volunteer Data">
                <group>
                    <field name="file" filename="filename"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import_file" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_import_volunteer_wizard" model="ir.actions.act_window">
        <field name="name">Import Volunteer Data</field>
        <field name="res_model">res.users.import.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>
